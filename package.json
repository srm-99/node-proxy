{
  "name": "node-proxy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev:run": "nodemon --watch build build/index.js",
    "dev:transpile": "esbuild ./src/index.ts --platform=node --bundle --outdir=build --watch",
    "dev": "yarn run concurrently -k --kill-others-on-fail \"yarn dev:run\" \"yarn dev:transpile\"",
    "build": "esbuild ./src/index.ts --platform=node --bundle --minify --outdir=build",
    "start": "node ./build/index.js"
  },
  "keywords": [
    "node"
  ],
  "engines": {
    "node": "16",
    "npm": "please-use-yarn",
    "yarn": ">= 1.15.1"
  },
  "author": "SRM99",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "https": "^1.0.0",
    "nodemon": "^2.0.20",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "concurrently": "^7.5.0",
    "esbuild": "^0.15.13"
  }
}
